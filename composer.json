{
    "name": "slon/http-protocol",
    "require": {
        "slon/streams": "dev-main",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "Slon\\Http\\Protocol\\": "src/"
        },
        "files": [
            "functions/assert_server_file.php",
            "functions/normalize_server_files.php",
            "functions/parse_server_headers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Slon\\Http\\Protocol\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "slon/streams": {
            "type": "vcs",
            "url": "https://github.com/gigamel/slon-streams.git"
        }
    }
}
